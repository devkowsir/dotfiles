{
    "editor.fontFamily": "JetBrainsMonoMedium Nerd Font Mono",
    "editor.fontLigatures": true,
    "editor.fontWeight": "500",
    "editor.insertSpaces": true,
    "editor.linkedEditing": true,
    "explorer.confirmDelete": false,
    "keyboard.dispatch": "keyCode",
    "liveSassCompile.settings.formats": [ { "format": "expanded", "extensionName": ".css", "savePath": "/dist/css", } ],
    "liveSassCompile.settings.generateMap": false,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "security.workspace.trust.untrustedFiles": "open",
    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindingsNonRecursive": [ { "before": [ "C-o" ], "after": ["Esc", "o"] } ],
    "vim.leader": " ",
    "vim.smartRelativeLine": true,
    "vim.sneak": true,
    "vim.sneakReplacesF": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": false,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "$HOME/.config/nvim/keybindings.vim",
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.editor.untitled.hint": "hidden",
}